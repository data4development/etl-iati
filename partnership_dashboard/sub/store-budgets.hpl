    </partitioning>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
// budgetdays = dateDiff(str2date(period_start, "yyyy-MM-dd"), str2date(period_end, "yyyy-MM-dd"), "d")
budgetdays = dateDiff(period_start, period_end, "d") + 1
yearstart_ = parseInt(yearstart)
yearend_ = parseInt(yearend)
thisyear_ = parseInt(thisyear)

if (yearstart_ == thisyear_ &amp;&amp; yearend_ == thisyear_) {
  bp = budgetdays
} else if (yearstart_ == thisyear_) {
  bp = daysinyear - daystart + 1
} else if (yearend_ == thisyear_) {
  bp = dayend
} else {
  bp = daysinyear
}

budgetportion = bp/budgetdays

if ( yearstart_ == thisyear_ ) { 
  budgetstart = period_start
} else {
  budgetstart = str2date( thisyear + "-01-01", "yyyy-MM-dd")
}

if (yearend_ == thisyear_) { 
  budgetend = period_end
} else {
  budgetend = str2date( thisyear + "-12-31", "yyyy-MM-dd")
}
</jsScript_script>
      </jsScript>
